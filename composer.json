{
  "name": "miaoxing/wechat-oa",
  "type": "miaoxing-plugin",
  "description": "Miaoxing WeChat official account plugin",
  "authors": [
    {
      "name": "twinh",
      "email": "twinhuang@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Miaoxing\\WechatOa\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\WechatOa\\": "tests"
    }
  },
  "require": {
    "miaoxing/plugin": "^0.8.2",
    "miaoxing/user": "^0.4.5",
    "miaoxing/wechat": "^0.1.6"
  },
  "require-dev": {
    "miaoxing/dev": "^8.1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 0.1.x-dev",
      "miaoxing/plugin": "miaoxing/plugin as 0.8.x-dev",
      "miaoxing/services": "miaoxing/services as 0.5.x-dev",
      "wei/wei": "twinh/wei as 0.12.x-dev",
      "miaoxing/wechat": "miaoxing/wechat as 0.1.x-dev"
    }
  }
}
